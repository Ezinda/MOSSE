Private Sub anular_Click()
On Error Resume Next
                
        DataGrid2.Bookmark = DataCombo1.SelectedItem
        If DataGrid2.Columns(6).Text = "" Then DataGrid2.Columns(6).Text = 0

        databonan.RecordSource = "select ordendepagoabonan.* from ordendepagoabonan where empresa = " & login.empresaact & " and inicioper = '" & login.iper & "' and nrorden = '" & DataCombo1.Text & "'"
        databonan.Refresh
        If databonan.Recordset.EOF = True Then
            mensa = MsgBox("Esta Orden pertenece a un Periodo Contable Anterior, ingrese al periodo correspondiente e intente nuevamente", vbCritical, "Error")
            Exit Sub
        End If
        databonan.Recordset.MoveFirst

        datbuscarecibo.RecordSource = "select ordendepago.* from ordendepago WHERE ordendepago.empresa = " & login.empresaact & " AND nrorden = '" & DataCombo1.Text & "' "
        datbuscarecibo.Refresh

        datmaestro.RecordSource = "select [Maestro Asientos].* from [Maestro Asientos] where empresa = " & login.empresaact & " and perinicial = '" & login.iper & "' and nroasiento = " & datbuscarecibo.Recordset.Fields("idasiento") & "  "
        datmaestro.Refresh

        mensa = MsgBox("Esta segura de anular esta Orden", vbYesNo, "Atencin")
        If mensa = vbYes Then
            If datmaestro.Recordset.EOF = True Then GoTo sigue
            idmaster = datmaestro.Recordset.Fields("idmasterasientos")
            datmaestro.Recordset.Delete adAffectCurrent
           
sigue:
            datrecibocobro.RecordSource = "select ordendepago.* from ordendepago where empresa = " & login.empresaact & " and nrorden = '" & DataCombo1.Text & "'"
            datrecibocobro.Refresh
            datrecibocobro.Recordset.Fields("anulado") = "S"
            datrecibocobro.Recordset.Fields("idasiento") = 0
            datrecibocobro.Recordset.UpdateBatch adAffectCurrent
            
            datinstru.RecordSource = "select ordendepagoasignacion.* from ordendepagoasignacion where empresa = " & login.empresaact & " and nuevaorden = '" & DataCombo1.Text & "' order by orden"
            datinstru.Refresh
  
            If datinstru.Recordset.EOF = True Then GoTo sigue0
            datinstru.Recordset.MoveFirst
            Do While Not datinstru.Recordset.EOF
                    ordencita = datinstru.Recordset.Fields("orden")
                    databonan1.RecordSource = "select ordendepagoabonan.* from ordendepagoabonan where empresa = " & login.empresaact & " and inicioper = '" & login.iper & "' and nrorden = '" & ordencita & "'"
                    databonan1.Refresh
                    databonan1.Recordset.Fields("importe") = databonan1.Recordset.Fields("importe") + datinstru.Recordset.Fields("importe")
                    databonan1.Recordset.UpdateBatch adAffectCurrent
                    datinstru.Recordset.Delete adAffectCurrent
                    datinstru.Recordset.MoveNext
            Loop
     
sigue0:
            Do While Not databonan.Recordset.EOF
                cli = databonan.Recordset.Fields("nomproveedor")
                tip = Left(databonan.Recordset.Fields("comprobante"), 3)
                num = Right(databonan.Recordset.Fields("comprobante"), 13)
                datlibrocompras.RecordSource = "select librocompras.* from librocompras where empresa = " & login.empresaact & " and proveedor = '" & cli & "' and tipocompr = '" & tip & "' and numcompr = '" & num & "'  "
                datlibrocompras.Refresh
                datlibrocompras.Recordset.Fields("saldo") = databonan.Recordset.Fields("importe") + databonan.Recordset.Fields("saldofactura")
                datlibrocompras.Recordset.UpdateBatch adAffectCurrent
                databonan.Recordset.MoveNext
            Loop
            
                Inicio.datauditoria.ConnectionString = login.conexiontotal
    
                Inicio.datauditoria.RecordSource = "select auditoria.* from auditoria"
                Inicio.datauditoria.Refresh
    
                Inicio.datauditoria.Recordset.AddNew
                Inicio.datauditoria.Recordset.Fields("fecha") = Date
                Inicio.datauditoria.Recordset.Fields("hora") = Str(Time)
                Inicio.datauditoria.Recordset.Fields("ventana") = "Anular Ordenes de Pago"
                Inicio.datauditoria.Recordset.Fields("accion") = "Anulacion Orden:" + DataCombo1.Text
                Inicio.datauditoria.Recordset.Fields("usuario") = login.usuarioactivo
                Inicio.datauditoria.Recordset.Fields("empresa") = login.empresaact
                Inicio.datauditoria.Recordset.UpdateBatch adAffectCurrent
                
                mensa = MsgBox("ORDEN ANULADA", vbInformation, "Informacin")
        End If

Exit Sub
errorfuera:
    mensa = MsgBox("Selecciones la Orden y anule", vbCritical, "Error")
    
End Sub

Private Sub Command4_Click()
Dim tabla As String
